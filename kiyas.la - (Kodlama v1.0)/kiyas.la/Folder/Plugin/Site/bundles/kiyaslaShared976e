function Search(){this.init=function(n){$(function(){$("#kiyasla").click(function(t){t.preventDefault();var i=$("#"+n.product1Id).val(),r=$("#"+n.product2Id).val();$.post(n.searchUrl,{Product1:i,Product2:r})});$('#searchForm input[type="text"]').watermarkAutocomplete({watermarkenabled:!0,watermarktext:n.enterAProductText,allowfreetext:!0,autocompletesource:function(t,i){$.ajax({url:n.searchAutoCompleteUrl,type:"POST",dataType:"json",data:{query:t.term,productType:$("#hiddenProductType").val()},success:function(n){i($.map(n,function(n){return{label:n.fullName,value:n.fullName,id:n.id,productType:n.productType}}))}})},autocompleteselect:function(n,t){$("#hiddenProductType").val(t.item.productType)}});$('#searchForm input[type="text"]').focus(function(){$(this).val()==n.enterAProductText&&$(this).val("")});$('#searchForm input[type="text"]').keyup(function(t){var i=$('#searchForm input[type="text"]').not(this).val();t.which==8&&(i==null||i==n.enterAProductText)&&$("#hiddenProductType").val("-1")})})}}$(function(){$(".popularComparisons div").click(function(){window.location.href=$(this).find("a").attr("href")});$(".popularComparisons div").mouseover(function(){$(".popularComparisons div").not(this).css("opacity",".70")});$(".popularComparisons").mouseout(function(){$(".popularComparisons div").css("opacity","1")})})