$(function(){$("#newArrivals").bjqs({animation:"slide",width:920,height:330,rotationSpeed:6500,showMarkers:!0,showControls:!1,centerMarkers:!0,useCaptions:!1});$(".compareLi").click(function(){window.location.href=$(this).find("a").attr("href")});$(".tiptipImage").tipTip({fadeIn:10,delay:100,defaultPosition:"top"});var n=0,t=0;$("a.newArrivalProdImg").colorbox({innerWidth:"1002px",innerHeight:"570px",scrolling:!1,iframe:!0,onOpen:function(){n=$("#newArrivals").css("z-index");$("#newArrivals").css("z-index","-1");t=$(".newSign").css("z-index");$(".newSign").css("z-index","-1")},onClosed:function(){$("#newArrivals").css("z-index",n);$(".newSign").css("z-index",t)}});$("a.newArrivalProdImg").click(function(){if($(this).attr("href").indexOf("?regular=1")==-1){var n=$(this).attr("href")+"?regular=1";$(this).attr("href",n)}})})